{
  "name": "cellcolabs",
  "colors": {
    "primary": "#0066CC",
    "secondary": "#00A86B",
    "accent": "#FF6B35",
    "text": {
      "primary": "#161616",
      "secondary": "#666666",
      "inverse": "#FFFFFF"
    },
    "background": {
      "primary": "#FFFFFF",
      "secondary": "#F8F9FA",
      "section": "#F5F5F5",
      "dark": "#161616"
    },
    "border": {
      "light": "#E0E0E0",
      "medium": "#CCCCCC"
    }
  },
  "typography": {
    "fontFamily": {
      "heading": "'Barlow', sans-serif",
      "body": "'Inter', sans-serif"
    },
    "fontSize": {
      "mobile": {
        "h1": "32px",
        "h2": "24px",
        "h3": "20px",
        "body": "16px",
        "small": "14px"
      },
      "desktop": {
        "h1": "48px",
        "h2": "36px",
